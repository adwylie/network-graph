<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="jar">

  <target name="compile">
    <javac srcdir="." excludes="**/Tests/*"/>
  </target>

  <target name="jar" depends="compile">
    <jar destfile="NetGraph.jar"
         basedir="."
         includes="**/*.class"
    	 excludes="**/Tests/*">
      <manifest>
        <attribute name="Main-Class" value="UI.NetworkGUI" />
      </manifest>
    </jar>
  </target>
  	
</project>